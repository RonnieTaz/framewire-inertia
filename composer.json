{
    "name": "ronnie/framewire-inertia",
    "description": "PSR-15 adapter for InertiaJS using Symfony HTTP Foundation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Framewire\\Inertia\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Vicent Simon",
            "email": "vicsimon.vs4@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/http-foundation": "^v5.4|^v6.0|v7.0"
    },
    "suggest": {
        "psr/container": "^2.0"
    },
    "require-dev": {
        "psr/container": "^2.0",
        "pestphp/pest": "^2.31",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
